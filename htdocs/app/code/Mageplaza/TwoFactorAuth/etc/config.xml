<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <mptwofactorauth>
            <module>
                <type>1</type>
                <user_guide>two-factors-authentication</user_guide>
            </module>
            <general>
                <enabled>1</enabled>
                <force_2fa>0</force_2fa>
                <trust_device>0</trust_device>
                <trust_time>30</trust_time>
            </general>
        </mptwofactorauth>
    </default>
</config>
